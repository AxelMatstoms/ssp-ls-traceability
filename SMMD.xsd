<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Category">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="GeneralInformation">
					<xs:annotation>
						<xs:appinfo xml:lang="en">
							<displayName>General Information</displayName>
							<description>The general information contains administrational and organizational model metadata and information that helps to classify the model within the product lifecycle context.</description>
						</xs:appinfo>
						<xs:documentation xml:lang="en"></xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Logistics">
								<xs:annotation>
									<xs:appinfo xml:lang="en">										
										<description>The object describes logistical meta data.</description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Type" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<description>The attribute distinguishes whether the following [name] are a model specification or a model documentation.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Sender" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<description>The attribute references to a person and organization object with the role "sender" that represents the sender of the [name]. (see General information > Person and organization > role).</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Recipient" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<description>The attribute references to a person and organization object with the role "recipient" that represents the recipient of the [name]. (see General information > Person and organization > role).</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="ModelDateTransferFormat">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Model Date Transfer Format</displayName>
										<description>The object describes the format that ist used to tranfer the model</description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Format" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<description>The attribute indicates the format that is used to transfer the model.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="FormatVersion" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Format Version</displayName>
													<description>The attributes indicates the format version that is used to transfer the model.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="ModelMetaData">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Model Meta Data</displayName>
										<description>The object contains the core model meta data.</description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Content" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<description>The attribute contains a description or the identification of the modelled real world items/object. The description and identification shall be as clear and unambiguous as possible for the recipient of the [name] with respect to the confidentiality of the IDs and information.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Purpose" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<description>The attribute describes the intended use of the transferred model, i.e. the attribute describes the simulation purpose for which the model is intended.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CreationDate" type="xs:date">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Creation Date</displayName>
													<description>The attribute indicates the date the model was created. This attribute is only applicable and mandatory for the documentation of an actually created and tranferred model. It is not applicable for the specification of a requested model.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Version" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<description>The attribute indicated the version of the model. This attribute is only applicable for the documentation of an actually created and tranferred model (see General information > Logistics > type). It is not applicable for the specification of a requested model.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="LifecycleStage" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Lifecycle Stage</displayName>
													<description>The attribute indicates the lifecycle stage the model content is associated with.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="IndividualLifecycleStage" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Individual Lifecycle Stage</displayName>
													<description>The attribute may be used in case the attribute lifecycle stage (see General Information > Model > lifecycle stage) with its predefined value entries does not provide an appropriate entry.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ConfidentialityLevel" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Confidentiality Level</displayName>
													<description>The attribute indicates the confidentality level of the model content is associated with.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="IndividualConfidentialityLevel" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Individual Confidentiality Level</displayName>
													<description>The attribute may be used in case the attribute lifecycle stage (see General Information > Model > confidentiality level) with its predefined value entries does not provide an appropriate entry.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="PersonsAndOrganizations">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Persons And Organizations</displayName>
										<description>The object contains organizational data regarding creator, sender and receiver.</description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="PersonAndOrganization" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Persons And Organization</displayName>
													<description>The object contains organizational data regarding creator, sender and receiver.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Role" type="xs:string">
														<xs:annotation>
															<xs:appinfo xml:lang="en">
																<description>The attribute specifies the role of the person described by the instatiated person object.</description>
															</xs:appinfo>
															<xs:documentation xml:lang="en"></xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Name" type="xs:string">
														<xs:annotation>
															<xs:appinfo xml:lang="en">
																<description>The attribute indicates the name and surname of the specified person.</description>
															</xs:appinfo>
															<xs:documentation xml:lang="en"></xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Organization" type="xs:string">
														<xs:annotation>
															<xs:appinfo xml:lang="en">
																<description>The attribute contains some information about the organization the specified person belongs to. The rules regarding the details of company and department information given are not binding. In case of a company internal datra transfer the department information might be sufficient enough, in case of a cross company data transfer the name of the company might be sufficient enough. Due to the sensitivity of organizational information this attribute is optional.</description>
															</xs:appinfo>
															<xs:documentation xml:lang="en"></xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="PhoneNumber" type="xs:string">
														<xs:annotation>
															<xs:appinfo xml:lang="en">
																<displayName>Phone Number</displayName>
																<description>The attribute contains the phone number of thespecified person. Due to the sensitivity of personal contact information this attribute is optional.</description>
															</xs:appinfo>
															<xs:documentation xml:lang="en"></xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="EmailAddress" type="xs:string">
														<xs:annotation>
															<xs:appinfo xml:lang="en">
																<displayName>Email Address</displayName>
																<description>The attribute contains the email address of the specified person. Due to the sensitivity of personal contact information this attribute is optional.</description>
															</xs:appinfo>
															<xs:documentation xml:lang="en"></xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="IntellectualPropertyRights">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Intellectual Property Rights</displayName>
										<description>The object contains information about the creators/senders intellectual property right for the model and its content.</description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="PropertyRights" type="xs:boolean">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Property Rights</displayName>
													<description>The attribute indicates whether or not the model is subject to intellectual property rights.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="LicenseType" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>License Type</displayName>
													<description>The attribute indicates the type of license for the usage of the model content and associated information. This attribute is only applicable in case the model and its content is associated with property rights (see General information > Intellectual property rights > property rights).</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="IndividualLicenseType" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Individual License Type</displayName>
													<description>The attribute indicates the type of license for the usage of the model content and associated information. This attribute may be used in case the attribute license type with ist predefined value entries (see General information > Intellectual property rights > license type) does not provide an appropriate entry. The individual license type could e.g. be a reference to an bilateral aggreement or an indiviual license model.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="AdditionallyRequiredLicenses" minOccurs="0">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Additionally Required Licenses</displayName>
										<description></description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AdditionalLicense" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Additionally Required License</displayName>
													<description></description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="License" type="xs:string">
														<xs:annotation>
															<xs:appinfo xml:lang="en">
																<description></description>
															</xs:appinfo>
															<xs:documentation xml:lang="en"/>
														</xs:annotation>
													</xs:element>
													<xs:element name="Comment" type="xs:string">
														<xs:annotation>
															<xs:appinfo xml:lang="en">
																<displayName></displayName>
																<description></description>
															</xs:appinfo>
															<xs:documentation xml:lang="en"/>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ModelImplementationInformation">
					<xs:annotation>
						<xs:appinfo xml:lang="en">
							<displayName>Model Implementation Information</displayName>
							<description>The model implementation information contain information about the tools and technologies used to implement the model</description>
						</xs:appinfo>
						<xs:documentation xml:lang="en"></xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ImplementationLanguage">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Implementation Language</displayName>
										<description>The object indecates the implementation language used to implement the model. Note that for a model the language can be specified in addition to a tool and/or a compiler or not. For example for a Dymola model, the implementation language is Modelica. The user can specify Dymola as a tool and Modelica as a language.</description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="LanguageName" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Language Name</displayName>
													<description>The attribute indicates the name of the implementation language.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="LanguageVersion" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Language Version</displayName>
													<description>The attributes indicates the specific version of the implementation language.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Compiler">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<description></description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"/>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="CompilerName" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Compiler Name</displayName>
													<description>The attribute indicated the name of the compiler for the model.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CompilerVersion" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Compiler Version</displayName>
													<description>The attribute indicated the version of the compiler for the model.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="LicenseType" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>License Type</displayName>
													<description></description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="ModellingTool">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Modelling Tool</displayName>
										<description></description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ToolName" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Tool Name</displayName>
													<description>The attribute indicates the name of the preprocessing tool for generating the model</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ToolVersion" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Tool Version</displayName>
													<description>The attribute indicates the version of the preprocessing tool for generating the model</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ModelExecutionInformation">
					<xs:annotation>
						<xs:appinfo xml:lang="en">
							<displayName>Model Execution Information</displayName>
							<description>The model execution information contain information about the technical environment in wich the model is intended processed and simulated. The technical environment may be different at the creators and users site. The different technical circumstances might have influences on the execution and simulation behaviour. Therefore these technical environment information are very significant für the usage of the model at the receivers site.</description>
						</xs:appinfo>
						<xs:documentation xml:lang="en"></xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ModelExecutionEnvironment">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Model Execution Environment</displayName>
										<description>The object contains the detailed information about the operating system parameters and other information related to the model execution environment. It helps the simulation architect to anticipate the integration of the model.</description>
									</xs:appinfo>
									<xs:documentation xml:lang="en"></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="OperatingSystem" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Operating System</displayName>
													<description>The attributeindicates the operation system, in case the model execution is technically restricted to a specific operation system or a list of operation systems. It may be empty in case of no restrictions.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CPUCoreNumber" type="xs:integer">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>CPU Core Number</displayName>
													<description>The attribut indicates the number of CPU cores needed to execute the model with sufficient performance. It may be empty in case the number of CPU cores is not significant.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="MassMemory" type="xs:float">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Mass Memory</displayName>
													<description>The attribute indicates the size of available mass memory during the execution of the model to intermediately store runtime data and results of the simulation. It may be empty in case the requirements about the mass memory are presumed not significant.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="RAMSize" type="xs:float">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>RAM Size</displayName>
													<description>The attribute indicates the size of available RAM during the execution of the model to execute the model with sufficient performance and to intermediately store runtime data and results of the simulation. It may be empty in case the requirements about the RAM size are presumed not significant.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="RAMBandWith" type="xs:float">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>RAM Band With</displayName>
													<description>The attribute indicates the available RAM band width during the execution of the model to process the execution with sufficient performance, and to intermediately store runtime data and results of the simulation. It may be empty in case the requirements about the RAM band width are presumed not significant.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="OSFunctions" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>OS Functions</displayName>
													<description>The attribute describes required and used operating system functionalities like copy,paste functionalities. It may be empty in case such functionalities are not significant.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CacheProtocol" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>Cache Protocol</displayName>
													<description></description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="HPCRequirements" type="xs:string">
											<xs:annotation>
												<xs:appinfo xml:lang="en">
													<displayName>HPC Requirements</displayName>
													<description>The attribute indicated the name of the solver that is used or required to execute the model.</description>
												</xs:appinfo>
												<xs:documentation xml:lang="en"></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="SolverSettings">
								<xs:annotation>
									<xs:appinfo xml:lang="en">
										<displayName>Solver Settings</displayName>
